pipeline{
    agent any
    tools {
        maven '3.8.6'
    }
    stages{
        stage ('Compile Stage') {
            steps{

            withMaven(maven: '3.8.6') {
                bat 'mvn clean compile'
            }
            }
        }
        stage ('Testing Stage') {
            steps{

            withMaven(maven: '3.8.6') {
                bat 'mvn test'
            }
            }
        }
        stage ('Deployment Stage') {
            steps{

            withMaven(maven: '3.8.6') {
                bat 'mvn deploy -DaltDeploymentRepository=internal.repo::default::file:///https://github.com/GiulianoMendez/Test/tree/main/untitled'
            }
            }
        }
        
    }
}